#PROGRAM MENAMPILKAN SISWA LULUS ATAU TIDAK LULUS
siswa_lulus = []
siswa_tidak_lulus = []
for i in range(5):
    nilai_siswa = float(input("Masukkan nilai siswa {}: ".format(i + 1)))
    if nilai_siswa >= 70:
        siswa_lulus.append("Siswa {}".format(i + 1))
    else:
        siswa_tidak_lulus.append("Siswa {}".format(i + 1)) 
print("\nSiswa yang lulus:", siswa_lulus)
print("Siswa yang tidak lulus:", siswa_tidak_lulus)